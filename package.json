{
  "name": "succinct-react-starter",
  "version": "1.0.0",
  "description": "A simple React starter kit inspired by Brian Holt",
  "main": "App.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js, jsx, css, json} \"",
    "test": "jest --silent",
    "testu": "jest --silent -u",
    "testw": "jest --silent --watch",
    "testc": "jest --silent --coverage",
    "lint": "tslint --project .",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --out-dir dist --public-url ./",
    "buildgh": "parcel build src/index.html --out-dir docs --public-url ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jenniferlynparsons/succinct-react-starter.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Jennifer Lyn Parsons",
  "license": "GNU GENERAL PUBLIC LICENSE",
  "bugs": {
    "url": "https://github.com/jenniferlynparsons/succinct-react-starter/issues"
  },
  "homepage": "https://github.com/jenniferlynparsons/succinct-react-starter#readme",
  "devDependencies": {
    "@types/reach__router": "^1.2.2",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.6.0",
    "sass": "^1.14.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "bulma": "^0.7.2",
    "emotion": "^9.2.12",
    "identity-obj-proxy": "^3.0.0",
    "petfinder-client": "0.0.3",
    "react": "^16.6.0",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.12",
    "react-loadable": "^5.5.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
